{
    "head0": {
        "type": "header",
        "description": "Wallpapers from alpha.wallhaven.cc"
    },
    "rwall-source":{
        "type": "combobox",
        "default": "100",
        "description": "Alpha Wallhaven.cc Source (MUST UPDATE CACHE AFTER CHANGE):",
        "options": {
            "Top Images": "top",
            "Latest Images": "latest",
            "Search (below)": "search",
            "Untagged": "untagged"
        }
    },
    "query-string": {
        "type": "entry",
        "default": "id:37",
        "description": "Word(s) to use when searching for wallpapers:"
    },
    "resolution-string": {
        "type": "entry",
        "default": "1920x1080",
        "description": "The resolution of the wallpaper:"
    },
    "cron-frequency-string": {
        "type": "entry",
        "default": "0 * * * *",
        "description": "Change Frequency (cron syntax):"
    },
    
    "update-cron-button": {
        "type": "button",
        "description": "Update Cron (click here if you modified the Change Frequency)",
        "callback": "updateCronButtonClick",
        "tooltip": "Update the cron job with these settings"
    },

    "purity-string":{
        "type": "combobox",
        "default": "100",
        "description": "Purity of images:",
        "options": {
            "Sfw": "100",
            "Sfw+Sketchy": "110",
            "Sketchy": "010",
            "Nsfw+Sketchy": "011",
            "Nsfw": "001",
            "All": "111"            
        }
    },

    "div1": {
        "type": "separator"
    },
   "head1": {
        "type": "header",
        "description": "Login Details for alpha.wallhaven.cc."
    },
    "username": {
        "type": "entry",
        "default": "",
        "description": "Username (register @ alpha.wallhaven.cc):"
    },
    "password": {
        "type": "entry",
        "default": "",
        "description": "Password (register @ alpha.wallhaven.cc):"
    },
    
    "div2": {
        "type": "separator"
    },
    "head2" : {
            "type": "header",
            "description": "Wallpapers from nik.bot.nu"
    },
    "nik-bot-nu-query":{
            "type": "entry",
            "default": "nature",
            "description": "Words to use when searching for wallpapers:"
    },
    "nik-bot-nu-purity":{
            "type": "combobox",
            "default": "adult-sketchy",
            "description":"Purity of the images:",
            "options": {
                    "On":"on",
                    "Off":"off",
                    "Sfw-Sketchy": "swf-sketchy",
                    "Sketchy":"sketchy",
                    "Nsfw-Sketchy":"adult-sketchy",
                    "Nsfw":"adult"
            }
    },
    "div3": {
        "type": "separator"
    },
    "head3": {
        "type": "header",
        "description": "Adultwalls.com settings"
    },
    "adultwalls-com-purity":{
            "type": "combobox",
            "default": "lingerie-models",
            "description":"Purity of the images:",
            "options": {
                    "Erotic":"erotic-wallpapers",
                    "Lingerie":"lingerie-models"
            }
    },
    "div4": {
        "type": "separator"
    },
    "head4": {
        "type": "header",
        "description": "Wallpaper Saving"
    },
    "wallpaper-save-directory": {
        "type": "entry",
        "default": "~/Pictures/rwall/",
        "description": "The location to save wallpapers to when the \"Save Wallpaper\" button is clicked:"
    },


    "div5": {
        "type": "separator"
    },
    "head5": {
        "type": "header",
        "description": "Select Wallpaper Service"
    },
    "wallpaper-exec": {
        "type": "radiogroup",
        "default": "rwall",
        "description": "Select Service:",
        "options" : {
                "Rwall": "rwall",
                "Nik.bot.nu": "nik.bot.nu",
                "Sexydesktopnudes.com": "sexydesktopnudes.com",
                "Adultwalls.com": "adultwalls.com",
                "Random": "random"
        }
    }


}
